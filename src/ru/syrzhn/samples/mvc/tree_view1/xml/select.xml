<?xml version="1.0" encoding="UTF-8"?>
<select-expression type="expression">
	<with type="keyword" syntax="WITH">
		<recursive type="keyword" syntax="RECURSIVE" />
		<common-table-expression type="exspression" />
	</with>
	<select-core type="expression">
		<select type="keyword" syntax="SELECT" />
		<distinct type="keyword" syntax="DISTINCT" />
		<all type="keyword" syntax="ALL" />
		<result-column type="expression">
			<column-name type="string" />
			<separator type="char">,</separator>
			<result-column />
		</result-column>
	</select-core>
</select-expression>